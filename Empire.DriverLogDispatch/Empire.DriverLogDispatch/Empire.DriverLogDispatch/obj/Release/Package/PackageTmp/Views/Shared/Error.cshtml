@model System.Web.Mvc.HandleErrorInfo 
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1>Error.</h1>
        <h2>@Model.Exception.Message</h2>


    </hgroup>
    <div>
        Stack Trace:

        @Model.Exception.StackTrace
    </div>
</body>
</html>
